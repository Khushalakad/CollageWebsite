    text-decoration: underline;
    margin: 8px;
    display:block;
    align-items: center;
    
}

.head{
    border: 2px solid navy;
    position: relative;
    top: 15%;
    left: 50%;
    transform: translate(-50%,-50%);
    width:90%;
    max-width: 750px;
    background: #fff;
    padding: 30px 40px; 

}
.head table{
    font-size: 19px;
    padding-right:5%;
